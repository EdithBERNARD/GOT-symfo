<form method="post" action="{{ path('app_backoffice_title_delete', {'id': title.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ title.id) }}">
    <br>
    <button class="btn btn-outline-light" >Delete</button>
</form>
